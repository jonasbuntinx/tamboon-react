{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "lib": ["DOM", "ESNext"],
    "noEmit": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES5"
  },
  "include": ["src", "typings"]
}
